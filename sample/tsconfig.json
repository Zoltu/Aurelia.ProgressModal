{
    "compilerOptions": {
        "module": "system",
        "noImplicitAny": true,
        "outDir": "../output/",
        "preserveConstEnums": true,
        "removeComments": false,
        "sourceMap": true,
        "target": "es5"
    },
    "filesGlob": [
        "./**/*.ts"
    ],
    "files": [
        "./app.ts",
        "./main.ts"
    ],
    "compileOnSave": false
}
